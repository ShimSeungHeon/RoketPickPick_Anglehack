<UserControl x:Class="RoketPickPick.Items.TabButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RoketPickPick.Items"
             mc:Ignorable="d" 
             d:DesignHeight="80" d:DesignWidth="200"
             MaxHeight="80"
             MinHeight="80"
             Background="DeepSkyBlue">
    <Grid>
        <Polygon Points="0,0 10,40 0,80" 
                 StrokeThickness="0"
                 Fill="White"/>

        <Polygon Points="190,0 200,40 200,0" 
                 StrokeThickness="0"
                 Fill="White"/>

        <Polygon Points="190,80 200,40 200,80" 
                 StrokeThickness="0"
                 Fill="White"/>
    </Grid>
</UserControl>
